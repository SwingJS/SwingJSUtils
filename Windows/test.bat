:NEXT
SHIFT
:LOOP
if "%1%"=="" GOTO DONE
if %1%==-ver GOTO VERSION
if %1%==-dir GOTO DIRECT
if %1%==-32 GOTO WIN32
if %1%==-64 GOTO WIN64
SET PROG=INSTALL%1%
GOTO NEXT

:VERSION
SHIFT
SET VERSION=%1%
ECHO %VERSION%
GOTO NEXT

:DIRECT
SHIFT
SET DIRECTORY=%1%
ECHO %DIRECTORY%
GOTO NEXT

:WIN64
SET WIN=64
ECHO %WIN%
GOTO NEXT

:WIN32
SET WIN=32
ECHO %WIN%
GOTO NEXT

:DONE
